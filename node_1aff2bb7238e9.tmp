var umPeriodo = [50,70,-30,-25]
function quantidadeDeMesesComPerda(umPeriodo) {
  var soma = 0
  for (var mes=0; mes< umPeriodo.length; mes++) {
    if(umPeriodo[mes] < 0){
        mes = umPeriodo[mes]
        soma = soma + mes
    }
  }
  return soma;
}

console.log(quantidadeDeMesesComPerda(umPeriodo))